{
  "name": "@w-vision/load-yumpu-api",
  "version": "0.0.2",
  "description": "A thin, Promise-returning helper for loading the Yumpu API.",
  "author": "w-vision AG <support@w-vision.ch>",
  "license": "MIT",
  "homepage": "https://www.w-vision.ch",
  "repository": {
    "type": "git",
    "url": "git://github.com/w-vision/load-yumpu-api.git"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "gzip-size-cli": "^3.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "clean": "rimraf '*.log'",
    "fix": "prettier-standard '*.js'",
    "lint": "standard '*.js'",
    "precommit": "lint-staged",
    "test": "browserify test.js | tape-run",
    "weight": "uglifyjs index.js --compress --mangle --toplevel | gzip-size"
  },
  "lint-staged": {
    "*.js": [
      "standard",
      "git add"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "api",
    "yumpu",
    "yumpu magazines",
    "yumpu-api",
    "loader",
    "digital magazines",
    "promise"
  ]
}
